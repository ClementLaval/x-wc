{
  "name": "x-wc",
  "version": "0.0.1",
  "description": "Enhance your static HTML declaratively with X-WC. Easily add dynamic features through web components for an enriched and extensible user experience.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
