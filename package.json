    "url": "https://github.com/ClementLaval/x-wc/issues"
  },
  "homepage": "https://github.com/ClementLaval/x-wc#readme",
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.18.0",
    "@types/jsdom": "^21.1.7",
    "eslint": "^9.18.0",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
